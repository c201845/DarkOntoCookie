###  http://www.semanticweb.org/OntoCookie#accosiatedWithConsent
:accosiatedWithConsent rdf:type owl:ObjectProperty ;
                       owl:inverseOf <https://w3id.org/GConsent#isConsentForDataSubject> ;
                       rdf:type owl:FunctionalProperty ;
                       rdfs:domain <https://w3id.org/GConsent#DataSubject> ;
                       rdfs:range <https://w3id.org/GConsent#Consent> .


###  http://www.semanticweb.org/OntoCookie#associatedWith
:associatedWith rdf:type owl:ObjectProperty ,
                         owl:FunctionalProperty ;
                rdfs:domain :Cookie ;
                rdfs:range <https://w3id.org/GConsent#Consent> ;
                rdfs:label "associatedWith" .


###  http://www.semanticweb.org/OntoCookie#dependson
:dependson rdf:type owl:ObjectProperty ;
           owl:inverseOf <https://w3id.org/GConsent#isContextForConsent> ;
           rdf:type owl:FunctionalProperty ;
           rdfs:domain :DarkPattern ;
           rdfs:range <https://w3id.org/GConsent#AlignmentOfPersonalData> ;
           rdfs:label "dependsOn" .


###  http://www.semanticweb.org/OntoCookie#generated
:generated rdf:type owl:ObjectProperty ,
                    owl:FunctionalProperty ;
           rdfs:domain :trackact:After_Consent ;
           rdfs:range <https://w3id.org/GConsent#Consent> ;
           rdfs:label "generated" .


###  http://www.semanticweb.org/OntoCookie#hasDomain
:hasDomain rdf:type owl:ObjectProperty ,
                    owl:FunctionalProperty ;
           rdfs:domain :Cookie ;
           rdfs:range :Domain ;
           rdfs:label "hasDomain" .


###  http://www.semanticweb.org/OntoCookie#hasPurpose
:hasPurpose rdf:type owl:ObjectProperty ;
            owl:inverseOf :isPurposeFor ;
            rdf:type owl:FunctionalProperty ;
            rdfs:domain :Cookie ;
            rdfs:range :Purpose ;
            rdfs:label "hasPurpose" .


###  http://www.semanticweb.org/OntoCookie#has_statuses
:has_statuses rdf:type owl:ObjectProperty ;
              owl:inverseOf <https://w3id.org/GConsent#hasAssociation> ;
              rdf:type owl:FunctionalProperty ;
              rdfs:domain :TrackingActivity ;
              rdfs:range <https://w3id.org/GConsent#Status> ;
              rdfs:label "hasStatuses" .


###  http://www.semanticweb.org/OntoCookie#hascontrolover
:hascontrolover rdf:type owl:ObjectProperty ,
                         owl:FunctionalProperty ;
                rdfs:domain <https://w3id.org/GConsent#AlignmentOfPersonalData> ;
                rdfs:range <https://w3id.org/GConsent#Processing> ;
                rdfs:label "hasControlOver" .


###  http://www.semanticweb.org/OntoCookie#includesDarkPattern
:includesDarkPattern rdf:type owl:ObjectProperty ;
                     owl:inverseOf :isDarkPatternIn ;
                     rdfs:domain :CookieDialog ;
                     rdfs:range :DarkPattern .


###  http://www.semanticweb.org/OntoCookie#isDarkPatternIn
:isDarkPatternIn rdf:type owl:ObjectProperty ,
                          owl:FunctionalProperty ;
                 rdfs:domain :DarkPattern ;
                 rdfs:range :CookieDialog .


###  http://www.semanticweb.org/OntoCookie#isDarkPatternOf
:isDarkPatternOf rdf:type owl:ObjectProperty ,
                          owl:FunctionalProperty ;
                 rdfs:domain :DarkPattern ;
                 rdfs:range :CookieDialog ;
                 rdfs:label "isDarkPatternOf" .


###  http://www.semanticweb.org/OntoCookie#isFoundin
:isFoundin rdf:type owl:ObjectProperty ,
                    owl:FunctionalProperty ;
           rdfs:domain :DarkPattern ;
           rdfs:range :CookieDialog ;
           rdfs:label "isFoundIn" .


###  http://www.semanticweb.org/OntoCookie#isPurposeFor
:isPurposeFor rdf:type owl:ObjectProperty ,
                       owl:FunctionalProperty ;
              rdfs:domain :Purpose ;
              rdfs:range :Cookie ;
              rdfs:label "isPurposeFor" .


###  http://www.semanticweb.org/OntoCookie#is_used_by_A
:is_used_by_A rdf:type owl:ObjectProperty ,
                       owl:FunctionalProperty ;
              rdfs:domain :trackact:After_Consent ;
              rdfs:range :CookieDialog ;
              rdfs:label "isUsedByB" .


###  http://www.semanticweb.org/OntoCookie#postConsentUsedBy
:postConsentUsedBy rdf:type owl:ObjectProperty ,
                            owl:FunctionalProperty ;
                   rdfs:domain :trackact:After_Consent ;
                   rdfs:range :CookieDialog .


###  http://www.semanticweb.org/OntoCookie#priorConsentUnsedBy
:priorConsentUnsedBy rdf:type owl:ObjectProperty ,
                              owl:FunctionalProperty ;
                     rdfs:domain :trackact:Before_Consent ;
                     rdfs:range :CookieDialog .


###  https://openscience.adaptcentre.ie/ontologies/GConsent/docs/ontology#hasStatus
<https://openscience.adaptcentre.ie/ontologies/GConsent/docs/ontology#hasStatus> rdf:type owl:ObjectProperty ;
                                                                                 rdfs:domain <https://w3id.org/GConsent#Consent> ;
                                                                                 rdfs:range <https://w3id.org/GConsent#Status> ;
                                                                                 rdfs:label "hasStatus" .


###  https://w3id.org/GConsent#atLocation
<https://w3id.org/GConsent#atLocation> rdf:type owl:ObjectProperty ,

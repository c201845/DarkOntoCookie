                                                     rdfs:domain <https://w3id.org/GConsent#Consent> ;
                                                     rdfs:range <https://w3id.org/GConsent#Processing> ;
                                                     rdfs:label "isPersonalDataForConsent" .


###  https://w3id.org/GConsent#isUpdatedConsentFor
<https://w3id.org/GConsent#isUpdatedConsentFor> rdf:type owl:ObjectProperty ,
                                                         owl:FunctionalProperty ;
                                                rdfs:domain <https://w3id.org/GConsent#Consent> ;
                                                rdfs:range :Environmental_Context ;
                                                rdfs:label "isUpdatedConsentFor" .


